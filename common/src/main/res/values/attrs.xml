<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ImageViewCustom">
        <attr name="place_holder" format="enum">
            <enum name="bank" value="5" />
            <enum name="product" value="4" />
            <enum name="wallet" value="3" />
            <enum name="store" value="2" />
            <enum name="user" value="1" />
            <enum name="blank" value="0" />
        </attr>
        <attr name="imageShape" format="enum">
            <enum name="undefine" value="0" />
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="radius" value="3" />
            <enum name="square" value="4" />
        </attr>
    </declare-styleable>

</resources>